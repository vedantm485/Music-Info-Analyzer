{
  "manifest_version": 3,
  "name": "Music Info Analyzer",
  "version": "1.0",
  "description": "Get details about currently playing music.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
